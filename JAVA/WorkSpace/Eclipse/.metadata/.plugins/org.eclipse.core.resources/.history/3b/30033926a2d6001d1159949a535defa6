package cn.edu.xidian.practise;


public class Point implements MyComparable{
	
	private int x;
	private int y;
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public double distance() {
		return Math.sqrt(x*x+y*y);
	}
	
	@Override
	public String toString() {
		return "("+x+","+y+")";
	}
}
